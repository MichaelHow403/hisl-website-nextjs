
# Example environment variables for HISL Website

# PostHog analytics key (public, can be exposed in client)
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key_here

# Sentry DSN for error monitoring (server-side)
SENTRY_DSN=your_sentry_dsn_here

# IntegAI backend API endpoint (for globe + simulation)
INTEGAI_API_URL=https://api.integai.local

# Optional: Feature flags
FEATURE_GLOBE=true
FEATURE_CHAT_SIMULATION=true
